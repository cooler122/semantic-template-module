<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:registry address="${zookeeper.segmentation.address}" check="false"></dubbo:registry>


    <dubbo:reference interface="com.cooler.semantic.facade.WordSegmentFacade" version="1.0-SNAPSHOT" id="wordSegmentFacade" timeout="2000"/>

    <dubbo:reference interface="com.cooler.semantic.facade.SemanticFacade" version="1.0-SNAPSHOT" id="semanticService" timeout="2000"/>
    <dubbo:reference interface="com.cooler.semantic.facade.DynamicConfigFacade" version="1.0-SNAPSHOT" id="dynamicConfigService" timeout="2000"/>

</beans>
